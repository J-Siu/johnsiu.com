{
	"manifest_version": 2,
	"name": "PinToMe",
	"version": "1.0.2",
	"description": "Pin site to home tab.",
	"author": "JSiu.Dev",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": [
		"storage"
	],
	"icons": [
		{
			"src": "icons/Icon-192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "icons/Icon-512.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	]
}